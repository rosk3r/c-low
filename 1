#include <stdio.h>
#include <iostream>
#include <string>
 using namespace std;
int main()
{
  int n, i = 0, ch = 0, nch = 0, sumch = 0, sumnch = 0;
  cin >> n;
  int *p = new int[n];
  for (i; i < n; i++)
  {
    cin >> p[i];
    if (p[i] % 2 == 0) sumch += p[i];
    else sumnch += p[i];
    if (p[i] % 2 == 0) ch++;
    else nch++;
  }
    if (ch > nch) for (i = 0; i < n; i++)
    {
        if (p[i] % 2 == 0) p[i] = sumnch;
    }
    if (nch > ch) for (i = 0; i < n; i++)
    {
        if (p[i] % 2 != 0) p[i] = sumnch;
    }
    for (i = 0; i < n; i++)
    {
        cout << p[i] <<' ';
    }
    cout << endl;
    system ("pause");
    return 0;
}
